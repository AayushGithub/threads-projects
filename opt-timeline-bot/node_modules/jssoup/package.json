{
  "name": "jssoup",
  "version": "0.0.15",
  "description": "JSSoup is a BeautifulSoup style HTML parser library.",
  "main": "./dist/lib/jssoup.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "test": "mocha --require @babel/register",
